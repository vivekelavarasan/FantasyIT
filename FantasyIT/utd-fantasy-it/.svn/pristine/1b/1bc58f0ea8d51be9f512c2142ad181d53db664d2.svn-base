<Page x:Class="FITPrototype.CreateTeam"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="500"
	Title="ManageTeam">

    <Grid Background="#FF273B57">
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="40" Margin="10,10,0,0" VerticalAlignment="Top" Width="240" Background="#FF273B57">
            <TextBlock x:Name="header" HorizontalAlignment="Left" Height="20" Margin="9,9,0,0" TextWrapping="Wrap" Text="Create Team" VerticalAlignment="Top" Width="220" FontSize="18"/>
        </Border>
        <DataGrid x:Name ="teamData" HorizontalAlignment="Left" Height="235" Margin="10,55,0,0" VerticalAlignment="Top" Width="335"/>
        <Button Content="Add." HorizontalAlignment="Left" Height="28" Margin="372,59,0,0" VerticalAlignment="Top" Width="105"/>
        <Button Content="Remove" Click="Remove_Click" HorizontalAlignment="Left" Height="28" Margin="372,109,0,0" VerticalAlignment="Top" Width="105"/>
        <Button Content="Form Team" Click="FormTeam_Click" HorizontalAlignment="Left" Height="28" Margin="372,160,0,0" VerticalAlignment="Top" Width="105"/>

    </Grid>
</Page>